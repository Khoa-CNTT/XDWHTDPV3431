{
  "name": "charity-system",
  "version": "1.0.0",
  "description": "Backend for Charity System using Node.js, Express, and Sequelize",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "lint": "eslint src",
    "audit": "npm audit"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.5",
    "@truffle/contract": "^4.6.31",
    "@truffle/hdwallet-provider": "^2.1.15",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0",
    "nodemailer": "^6.10.1",
    "paypal-rest-sdk": "^1.8.1",
    "sequelize": "^6.37.7",
    "web3": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "truffle": "^5.11.5"
  },
  "keywords": [
    "charity",
    "backend",
    "node.js",
    "express",
    "sequelize",
    "mysql"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "ISC",
  "engines": {
    "node": ">=20.17.0"
  }
